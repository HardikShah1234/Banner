<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomTextView">
        <attr name="collapsedLines" format="integer" />
        <attr name="animDuration" format="integer" />
        <attr name="animation_duration" format="integer" />
        <attr name="anchorTextColor" format="color" />
        <attr name="anchorPoint" format="integer" />
        <attr name="isExpanded" format="boolean" />
        <attr name="textMode" format="enum">
            <enum name="length" value="0" />
            <enum name="line" value="1" />
        </attr>
        <attr name="foregroundColor" format="color" />
        <attr name="ellipsizeText" format="string" />
        <attr name="isUnderlined" format="boolean" />
        <attr name="ellipsizeTextColor" format="color" />

    </declare-styleable>

    <attr name="customTextView" format="reference" />
</resources>
